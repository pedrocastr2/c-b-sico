
#include <iostream>  // Biblioteca para entrada e saÃ­da
using namespace std; // Evita ter que escrever std:: antes de cout e cin


int main() {
	int matriz[3][3];
	int i,c;


for(i=0;i<3;i++) {

		for (c = 0; c < 3; c++) {
			cin >> matriz [i][c];

	    }
		cout << "\n";
	}


	for(i=0;i<3;i++) {

		for (c = 0; c < 3; c++) {
			cout << matriz[c][i]<< " ";

	    }
		cout << "\n";
	}

	return 0; // Indica que o programa terminou com sucesso
}
